  "dependencies": {
    "esbuild": "^0.23.0",
    "josh_js_util": "^0.1.25",
    "material-symbols": "^0.22.2",
    "rtds-core": "^0.0.19",
    "typescript": "^5.5.4",
    "vitest": "^1.4.0"
  },
  "devDependencies": {
    "pureimage": "^0.4.13"
  }
}
