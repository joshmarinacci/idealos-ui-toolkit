{
  "name": "canvas-ui-toolkit",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "prep": "cp index.html build",
    "esbuild-serve": "esbuild src/index.ts --bundle --outdir=build --watch --serve --servedir=build",
